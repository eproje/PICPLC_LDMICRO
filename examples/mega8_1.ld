LDmicro0.1
MICRO=Atmel AVR ATmega8 28-PDIP
CYCLE=10000
CRYSTAL=4000000
BAUD=9600
COMPILED=G:\ELT_Soft\pic-plc_LDMICRO\mega8_1.hex

IO LIST
    Xin1 at 4
    Yout1 at 5
    Yout2 at 6
END

PROGRAM
RUNG
    CONTACTS Xin1 0
    TON Tt1 300000
    COIL Yout1 0 0 0
END
RUNG
    CONTACTS Yout1 0
    TON Tt2 300000
    COIL Yout2 0 0 0
END
RUNG
    MASTER_RELAY
END
RUNG
    UART_RECV mer
END
RUNG
    CONTACTS Yout2 0
    FORMATTED_STRING mer 13 118 97 108 117 101 58 32 92 51 92 114 92 110
END
